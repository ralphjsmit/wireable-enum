{
    "name": "ralphjsmit/wireable-enum",
    "description": "Make enums wireable.",
    "keywords": [
        "ralphjsmit",
        "laravel",
        "wireable-enum"
    ],
    "homepage": "https://github.com/ralphjsmit/wireable-enum",
    "license": "MIT",
    "authors": [
        {
            "name": "Ralph J. Smit",
            "email": "rjs@ralphjsmit.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^8.73"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10",
        "orchestra/testbench": "^6.22",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "RalphJSmit\\WireableEnum\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RalphJSmit\\WireableEnum\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
